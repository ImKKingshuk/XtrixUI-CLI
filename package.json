{
  "name": "xtrixui-cli",
  "version": "1.0.0",
  "type": "module",
  "description": "A CLI tool for scaffolding XtrixUI projects with ease.",
  "bin": {
    "xtrixui": "./bin/xtrixui.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "bin",
    "dist",
    "src",
    "templates"
  ],
  "scripts": {
    "build": "rslib build",
    "dev": "rslib build --watch",
    "start": "node ./bin/xtrixui.js",
    "format": "prettier --write .",
    "lint": "eslint .",
    "test": "vitest run"
  },
  "author": "Kingshuk Mondal",
  "license": "GPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "https://github.com/ImKKingshuk/XtrixUI-CLI.git"
  },
  "bugs": {
    "url": "https://github.com/ImKKingshuk/XtrixUI-CLI/issues"
  },
  "homepage": "https://github.com/ImKKingshuk/XtrixUI-CLI#readme",
  "keywords": [
    "cli",
    "xtrixui-cli",
    "xtrixui"
  ],
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@rslib/core": "^0.1.2",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.10.1",
    "@types/prompts": "^2.4.9",
    "eslint": "^9.16.0",
    "globals": "^15.13.0",
    "prettier": "^3.4.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.17.0",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "execa": "^9.5.1",
    "fs-extra": "^11.2.0",
    "inquirer": "^12.1.0",
    "prompts": "^2.4.2"
  }
}
